{
  "project": {
    "version": "1.0.0-beta.18",
    "name": "hf-pc-test"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "./assets/css/bootstrap/css/bootstrap.min.css",
        "./assets/css/swiper/swiper.css",
        "./assets/css/sweetalert/sweetalert.css",
        "./assets/css/iCheck/custom.css",
        "./assets/css/datetimepicker/bootstrap-datetimepicker.min.css",
        "styles.scss"
      ],
      "scripts": [
        "./assets/js/jquery-2.1.1.js",
        "./assets/js/swiper/swiper.js",
        "./assets/js/sweetalert/sweetalert.min.js",
        "./assets/js/iCheck/icheck.min.js",
        "./assets/js/datetimepicker/bootstrap-datetimepicker.js",
        "./assets/js/datetimepicker/locales/bootstrap-datetimepicker.zh-CN.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
